<div class="page-header">
  <h1>Course #<%= @course.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Department</dt>
  <dd><%= @course.department %></dd>

  <dt>Number</dt>
  <dd><%= @course.number %></dd>

  <dt>Title</dt>
  <dd><%= @course.title %></dd>

  <dt>Meetings</dt>
  <dd>
    <% Meeting.where(:course_id => @course.id).each do |meeting|%>
    <%= meeting.held_on %>
    <%end %>
  </dd>

</dl>


